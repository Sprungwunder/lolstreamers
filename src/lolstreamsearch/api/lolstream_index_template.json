{
  "index_patterns": [
    "lolstreamsearch_yt_videos*"
  ],
  "template": {
    "settings": {
      "auto_expand_replicas": "0-1"
    },
    "mappings": {
      "properties": {
        "title": {
          "type": "text"
        },
        "description": {
          "type": "text"
        },
        "champion": {
          "type": "search_as_you_type",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        },
        "lane": {
          "type": "keyword"
        },
        "lol_version": {
          "type": "keyword"
        },
        "team_champions": {
          "type": "keyword"
        },
        "opponent_champion": {
          "type": "keyword"
        },
        "opponent_team_champions": {
          "type": "keyword"
        },
        "published_at": {
          "type": "date"
        },
        "runes": {
          "type": "keyword"
        },
        "champion_items": {
          "type": "keyword"
        },
        "video_url": {
          "type": "keyword"
        },
        "streamer": {
          "type": "keyword"
        }
      }
    }
  }
}
